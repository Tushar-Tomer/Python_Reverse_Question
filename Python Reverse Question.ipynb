     "text": [
      "2 4 6 8 10\n",
      "10 8 6 4 2 "
     ]
    }
   ],
   "source": [
    "arr = list(map(int, input().rstrip().split()))\n",
    "arr.reverse()\n",
    "for i in arr:\n",
